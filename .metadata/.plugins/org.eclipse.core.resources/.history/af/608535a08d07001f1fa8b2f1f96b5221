package com.kh.test.main;

import java.util.ArrayList;
import java.util.Arrays;

public class 리스트예제 {
	public static void main(String[] args) {
		
		// ArrayList 생성
		ArrayList<String> 리스트 = new ArrayList<>(Arrays.asList("바나나", "오렌지"));
		
		// 값 추가   ▼                                                  ▲
		리스트.add("사과");
		
		System.out.println(리스트);
	}
}
