package com.kh.test.main.oop;

public class Main {

	// 최종 출력 및 실행 할 메인 메서드로, 내가 정말 실행하고자 하는 파일과 코드만 작성하는 공간이다.
	public static void main(String[] args) {
		
		Dog 댕댕 = new Dog();
		Dog 마루 = new Dog();
		Dog 시바 = new Dog();
		
		// 인터페이스 메서드에 작성한 makeSound 를, Dog 에서 재사용한 소리로 변경한 것 출력하기
		댕댕.makeSound();
		마루.makeSound();
		시바.makeSound();
		
	}
}
